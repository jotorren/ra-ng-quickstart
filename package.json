{
  "name": "ra-ng-qs",
  "version": "2.0.0-rc14",
  "description": "ra-ng project quickstart",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server -c liteserver.json",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "tslint": "tslint -c tslint.json src/app/**/*.ts",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "e2e": "protractor protractor.config.js --kill-others --success first",
    "webdriver:update": "webdriver-manager update",
    "karma": "karma start karma.conf.js"
  },
  "keywords": [
    "angular2",
    "ra-ng"
  ],
  "author": "ICT Analysis, Architecture & Design",
  "license": "Apache-2.0",
  "dependencies": {
    "ra-ng": "2.0.0-rc15",
    "@angular/common": "2.1.2",
    "@angular/compiler": "2.1.2",
    "@angular/core": "2.1.2",
    "@angular/forms": "2.1.2",
    "@angular/http": "2.1.2",
    "@angular/platform-browser": "2.1.2",
    "@angular/platform-browser-dynamic": "2.1.2",
    "@angular/router": "3.1.2",
    "@angular/upgrade": "2.1.2",
    "angular2-in-memory-web-api": "0.0.21",
    "angular2-jwt": "0.1.25",
    "angular2-uuid": "^1.1.0",
    "bootstrap": "^3.3.7",
    "cachefactory": "1.5.1",
    "core-js": "^2.4.1",
    "crypto-js": "^3.1.8",
    "lodash": "^4.16.6",
    "log4javascript": "^1.4.15",
    "ng2-translate": "3.1.2",
    "primeng": "^1.0.0-rc.4",
    "primeui": "^4.1.15",
    "quill": "^1.1.5",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "^0.19.41",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
    "typescript": "^2.0.9",
    "tslint": "3.15.1",
    "codelyzer": "~1.0.0-beta.3",
    
    "concurrently": "^3.1.0",
    "ts-node": "1.7.0",
    "lite-server": "^2.2.2",

    "jasmine-core": "^2.5.2",
    "jasmine-node": "^1.14.5",
    "jasmine-spec-reporter": "2.7.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "remap-istanbul": "^0.7.0",
    "protractor": "^4.0.11",
    "canonical-path": "^0.0.2",
    "webdriver-manager": "10.2.8",
    "rimraf": "^2.5.4",

    "@types/cachefactory": "^1.4.4",
    "@types/core-js": "^0.9.34",
    "@types/crypto-js": "^3.1.32",
    "@types/jasmine": "^2.5.37",
    "@types/selenium-webdriver": "^2.53.32",
    "@types/lodash": "^4.14.38",
    "@types/log4javascript": "^1.4.31",
