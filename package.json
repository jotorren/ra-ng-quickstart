{
  "name": "ra-ng-qs",
  "version": "2.0.0-rc22",
  "description": "ra-ng project quickstart",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server -c liteserver.json",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "tslint": "tslint -c tslint.json src/app/**/*.ts",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "e2e": "protractor protractor.config.js --kill-others --success first",
    "webdriver:update": "webdriver-manager update",
    "karma": "karma start karma.conf.js"
  },
  "keywords": [
    "angular2",
    "ra-ng"
  ],
  "author": "ICT Analysis, Architecture & Design",
  "license": "Apache-2.0",
  "dependencies": {
    "ra-ng": "2.0.0-rc22",
    "@angular/common": "^2.4.1",
    "@angular/compiler": "^2.4.1",
    "@angular/core": "^2.4.1",
    "@angular/forms": "^2.4.1",
    "@angular/http": "^2.4.1",
    "@angular/platform-browser": "^2.4.1",
    "@angular/platform-browser-dynamic": "^2.4.1",
    "@angular/router": "^3.4.1",
    "cachefactory": "1.5.1",
    "crypto-js": "^3.1.9-1",
    "lodash": "^4.17.3",
    "log4javascript": "^1.4.15",
    "ng2-translate": "^5.0.0",
    "primeng": "^1.1.2",
    "rxjs": "^5.0.2"
  },
  "devDependencies": {
    "typescript": "^2.1.4",
    "tslint": "^4.2.0",
    "codelyzer": "^2.0.0-beta.4",
    
    "bootstrap": "^3.3.7",
    "primeui": "^4.1.15",
    "quill": "^1.1.8",
    "reflect-metadata": "^0.1.9",
    "systemjs": "^0.19.41",
    "zone.js": "^0.7.4",
    
    "concurrently": "^3.1.0",
    "ts-node": "^2.0.0",
    "lite-server": "^2.2.2",

    "jasmine-core": "^2.5.2",
    "jasmine-node": "^1.14.5",
    "jasmine-spec-reporter": "^2.7.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "remap-istanbul": "^0.8.4",
    "protractor": "^4.0.14",
    "canonical-path": "^0.0.2",
    "webdriver-manager": "^11.1.0",
    "rimraf": "^2.5.4",

    "@types/cachefactory": "^1.4.4",
    "@types/core-js": "^0.9.35",
    "@types/crypto-js": "^3.1.32",
    "@types/jasmine": "^2.5.40",
    "@types/selenium-webdriver": "^2.53.38",
    "@types/lodash": "^4.14.45",
    "@types/log4javascript": "^1.4.31",
    "@types/node": "^6.0.55",
    "@types/quill": "^0.0.29"
  }
}
